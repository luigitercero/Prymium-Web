{
  "compilerOptions": {
    "baseUrl": "src",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "paths": {
      "@hooks/*": [
        "hooks/*"
      ],
      "@component/*": [
        "component/*"
      ],
      "@context/*": [
        "context/*"
      ],
    },
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports":true

  }
}