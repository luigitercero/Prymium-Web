{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es2020",
    "experimentalDecorators": false,
    "baseUrl": "src",
    "jsx": "react",
    "paths": {
      "@hooks/*": [
        "hooks/*"
      ],
      "@component/*": [
        "component/*"
      ],
      "@context/*": [
        "context/*"
      ],
    }
  }
}