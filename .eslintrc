{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "arrow-body-style": ["warning", "as-needed", { "requireReturnForObjectLiteral": true }],
    // eslint-disable-next-line react/jsx-props-no-spreading
    "react/jsx-props-no-spreading": [0,
      {
        "html": "enforce", //| "enforce"
        "custom": "ignore", // "enforce",
        "explicitSpread": "ignore", // | "enforce",
        "exceptions": "error" //[<string>]
      }
    ],
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "import/imports-first": [
      "error",
      "absolute-first"
    ],
    "import/newline-after-import": "error"
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true
  },
  "parser": "babel-eslint"
}